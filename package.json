{
  "name": "blinktrade-kycform",
  "description": "BlinkTrade KYC Form",
  "scripts": {
    "start": "cross-env NODE_ENV=production BABEL_ENV=node node index.js",
    "dev": "cross-env NODE_ENV=development BABEL_ENV=node node index.js",
    "build": "cross-env NODE_ENV=production webpack -p",
    "lint": "$(npm bin)/eslint app",
    "flow": "flow",
    "manage:translations": "$(npm bin)/babel app --plugins react-intl app/**/*.js && babel-node ./app/utils/translationRunner.js",
    "deploy": "gcloud app deploy"
  },
  "engines": {
    "node": ">=6.11.0",
    "npm": "4.6.1"
  },
  "dependencies": {
    "@google-cloud/storage": "0.3.0",
    "aphrodite": "^1.2.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "1.15.2",
    "cross-env": "^3.1.3",
    "dropbox": "^2.5.0",
    "express": "4.16.2",
    "flow-bin": "^0.56.0",
    "intl": "^1.2.5",
    "json-loader": "^0.5.4",
    "multer": "1.3.0",
    "node-fetch": "^1.6.3",
    "qs": "^6.5.1",
    "ramda": "^0.24.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-signature-pad": "0.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-form": "^7.1.0",
    "redux-thunk": "^2.2.0",
    "sjcl": "^1.0.6",
    "webpack": "3.6.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-isomorphic-tools": "^2.6.4"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "react-intl-translations-manager": "^5.0.1"
  }
